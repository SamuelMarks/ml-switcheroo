{
  "DataLoader": {
    "description": "Foundational PyTorch Data Loader. Explicitly unmapped to trigger Escape Hatch.",
    "std_args": [
      "dataset",
      "batch_size",
      "shuffle",
      "sampler"
    ],
    "variants": {
      "torch": {
        "api": "torch.utils.data.DataLoader",
        "args": {
          "dataset": "dataset",
          "batch_size": "batch_size",
          "shuffle": "shuffle",
          "sampler": "sampler"
        }
      },
      "jax": null
    }
  },
  "TensorDataset": {
    "description": "Simple dataset wrapping tensors.",
    "std_args": [
      "tensors"
    ],
    "variants": {
      "torch": {
        "api": "torch.utils.data.TensorDataset",
        "args": {
          "tensors": "tensors"
        }
      },
      "jax": null
    }
  },
  "no_grad": {
    "description": "Context manager to disable gradient calculation.",
    "std_args": [],
    "variants": {
      "torch": {
        "api": "torch.no_grad"
      },
      "jax": {
        "api": "jax.lax.stop_gradient",
        "requires_plugin": "context_to_function_wrap"
      }
    }
  },
  "save": {
    "description": "Serialization utility. IO is often framework specific.",
    "std_args": [
      "obj",
      "f"
    ],
    "variants": {
      "torch": {
        "api": "torch.save"
      },
      "jax": {
        "api": "orbax.checkpoint.save",
        "requires_plugin": "io_handler"
      }
    }
  },
  "load": {
    "description": "Deserialization utility.",
    "std_args": [
      "f"
    ],
    "variants": {
      "torch": {
        "api": "torch.load"
      },
      "jax": {
        "api": "orbax.checkpoint.restore",
        "requires_plugin": "io_handler"
      }
    }
  },
  "manual_seed": {
    "description": "Seeding RNG. Maps to standard numpy seed often.",
    "std_args": [
      "seed"
    ],
    "variants": {
      "torch": {
        "api": "torch.manual_seed"
      },
      "jax": {
        "api": "numpy.random.seed",
        "args": {
          "seed": "seed"
        }
      }
    }
  },
  "device": {
    "description": "Device placement object.",
    "std_args": [
      "type"
    ],
    "variants": {
      "torch": {
        "api": "torch.device"
      },
      "jax": {
        "api": "jax.devices",
        "requires_plugin": "device_allocator"
      }
    }
  },
  "is_tensor": {
    "description": "Type check helper.",
    "std_args": [
      "obj"
    ],
    "variants": {
      "torch": {
        "api": "torch.is_tensor"
      },
      "jax": {
        "api": "lambda x: hasattr(x, '__array__')",
        "transformation_type": "inline_lambda"
      }
    }
  },
  "set_printoptions": {
    "description": "Formatting utility for printing tensors.",
    "std_args": [
      "precision"
    ],
    "variants": {
      "torch": {
        "api": "torch.set_printoptions"
      },
      "jax": {
        "api": "numpy.set_printoptions"
      }
    }
  },
  "init_process_group": {
    "description": "Distributed Training initialization.",
    "std_args": [
      "backend",
      "rank",
      "world_size"
    ],
    "variants": {
      "torch": {
        "api": "torch.distributed.init_process_group"
      },
      "jax": null
    }
  },
  "eval": {
    "description": "Sets the module in evaluation mode.",
    "std_args": [],
    "variants": {
      "torch": {
        "api": "torch.nn.Module.eval"
      },
      "jax": {
        "api": "None",
        "requires_plugin": "capture_eval_state"
      }
    }
  },
  "train": {
    "description": "Sets the module in training mode.",
    "std_args": [
      "mode"
    ],
    "variants": {
      "torch": {
        "api": "torch.nn.Module.train"
      },
      "jax": {
        "api": "None",
        "requires_plugin": "capture_eval_state"
      }
    }
  },
  "add_": {
    "description": "In-place addition.",
    "std_args": [
      "x",
      "other",
      "alpha"
    ],
    "variants": {
      "torch": {
        "api": "torch.Tensor.add_"
      },
      "jax": {
        "api": "jax.numpy.add",
        "requires_plugin": "unroll_inplace_ops"
      }
    }
  },
  "nnx_Param": {
    "description": "Flax NNX Parameter definition.",
    "std_args": [
      "value"
    ],
    "variants": {
      "jax": {
        "api": "flax.nnx.Param"
      },
      "torch": {
        "api": "torch.nn.Parameter",
        "requires_plugin": "nnx_param_to_torch"
      }
    }
  },
  "dropout": {
    "description": "Stochastic Dropout.",
    "std_args": [
      "input",
      "p",
      "training",
      "inplace"
    ],
    "variants": {
      "torch": {
        "api": "torch.nn.functional.dropout"
      },
      "jax": {
        "api": "jax.random.bernoulli",
        "requires_plugin": "inject_prng"
      }
    }
  },
  "add": {
    "description": "Add with Alpha scaling support.",
    "std_args": [
      "x",
      "y",
      "alpha"
    ],
    "variants": {
      "torch": {
        "api": "torch.add",
        "requires_plugin": "recompose_alpha"
      },
      "jax": {
        "api": "jax.numpy.add",
        "requires_plugin": "decompose_alpha"
      }
    }
  },
  "cuda_is_available": {
    "description": "Checks if a CUDA device is available.",
    "std_args": [],
    "variants": {
      "torch": {
        "api": "torch.cuda.is_available"
      },
      "jax": {
        "api": "jax.devices",
        "requires_plugin": "cuda_is_available"
      }
    }
  }
}
