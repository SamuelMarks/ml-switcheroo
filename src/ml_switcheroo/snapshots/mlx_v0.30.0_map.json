{
  "__framework__": "mlx",
  "imports": {
    "torch.nn": {
      "alias": "nn",
      "root": "mlx",
      "sub": "nn"
    }
  },
  "mappings": {
    "Abs": {
      "api": "mx.abs"
    },
    "Adagrad": {
      "api": "mlx.optimizers.Adagrad",
      "args": {
        "lr": "learning_rate"
      },
      "requires_plugin": "mlx_optimizer_init"
    },
    "Adam": {
      "api": "mlx.optimizers.Adam",
      "args": {
        "lr": "learning_rate"
      },
      "requires_plugin": "mlx_optimizer_init"
    },
    "AdamW": {
      "api": "mlx.optimizers.AdamW",
      "args": {
        "lr": "learning_rate"
      },
      "requires_plugin": "mlx_optimizer_init"
    },
    "Linear": {
      "api": "mlx.nn.Linear",
      "args": {
        "in_features": "input_dims",
        "out_features": "output_dims"
      }
    },
    "Mean": {
      "api": "mx.mean"
    },
    "RMSprop": {
      "api": "mlx.optimizers.RMSprop",
      "args": {
        "lr": "learning_rate"
      },
      "requires_plugin": "mlx_optimizer_init"
    },
    "SGD": {
      "api": "mlx.optimizers.SGD",
      "args": {
        "lr": "learning_rate"
      },
      "requires_plugin": "mlx_optimizer_init"
    },
    "permute_dims": {
      "api": "mx.transpose",
      "requires_plugin": "pack_varargs"
    },
    "step": {
      "requires_plugin": "mlx_optimizer_step"
    },
    "zero_grad": {
      "requires_plugin": "mlx_zero_grad"
    }
  }
}