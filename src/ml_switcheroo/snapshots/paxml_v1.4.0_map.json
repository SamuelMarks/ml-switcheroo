{
  "__framework__": "paxml",
  "imports": {
    "praxis.layers": {
      "alias": "pl",
      "root": "praxis",
      "sub": "layers"
    }
  },
  "mappings": {
    "Abs": {
      "api": "jnp.abs"
    },
    "AdaBelief": {
      "api": "optax.adabelief",
      "requires_plugin": "optimizer_constructor"
    },
    "Adadelta": {
      "api": "optax.adadelta"
    },
    "Adafactor": {
      "api": "optax.adafactor"
    },
    "Adagrad": {
      "api": "optax.adagrad",
      "requires_plugin": "optimizer_constructor"
    },
    "Adam": {
      "api": "optax.adam",
      "requires_plugin": "optimizer_constructor"
    },
    "AdamW": {
      "api": "optax.adamw",
      "requires_plugin": "optimizer_constructor"
    },
    "Adamax": {
      "api": "optax.adamax"
    },
    "Adamw": {
      "api": "optax.adamw"
    },
    "Batchnorm": {
      "api": "praxis.layers.BatchNorm"
    },
    "Dotproductattention": {
      "api": "praxis.layers.DotProductAttention"
    },
    "Dropout": {
      "api": "praxis.layers.Dropout"
    },
    "Einsum": {
      "api": "jnp.einsum",
      "requires_plugin": "einsum_normalizer"
    },
    "Elu": {
      "api": "praxis.layers.ELU"
    },
    "Embedding": {
      "api": "praxis.layers.Embedding"
    },
    "Exp": {
      "api": "praxis.layers.activations.Exp"
    },
    "Fromage": {
      "api": "optax.fromage"
    },
    "Gelu": {
      "api": "praxis.layers.GELU"
    },
    "Groupnorm": {
      "api": "praxis.layers.GroupNorm"
    },
    "Identity": {
      "api": "praxis.layers.Identity"
    },
    "LBFGS": {
      "api": "optax.lbfgs",
      "requires_plugin": "optimizer_constructor"
    },
    "Lamb": {
      "api": "optax.lamb"
    },
    "Layernorm": {
      "api": "praxis.layers.LayerNorm"
    },
    "LeakyRelu": {
      "api": "praxis.layers.LeakyReLU"
    },
    "Leakyrelu": {
      "api": "praxis.layers.LeakyReLU"
    },
    "Linear": {
      "api": "praxis.layers.Linear"
    },
    "Lion": {
      "api": "optax.lion"
    },
    "Nadam": {
      "api": "optax.nadam"
    },
    "Novograd": {
      "api": "optax.novograd"
    },
    "Optional": {
      "api": "praxis.base_layer.Optional"
    },
    "RMSprop": {
      "api": "optax.rmsprop",
      "requires_plugin": "optimizer_constructor"
    },
    "Relu": {
      "api": "praxis.layers.ReLU"
    },
    "Rmsnorm": {
      "api": "praxis.layers.RmsNorm"
    },
    "SGD": {
      "api": "optax.sgd",
      "requires_plugin": "optimizer_constructor"
    },
    "Sequential": {
      "api": "praxis.layers.Sequential"
    },
    "Sgd": {
      "api": "optax.sgd"
    },
    "Sigmoid": {
      "api": "praxis.layers.Sigmoid"
    },
    "Silu": {
      "api": "praxis.layers.SiLU"
    },
    "Softplus": {
      "api": "praxis.layers.activations.Softplus"
    },
    "Swish": {
      "api": "praxis.layers.Swish"
    },
    "Tanh": {
      "api": "praxis.layers.Tanh"
    },
    "Yogi": {
      "api": "optax.yogi",
      "requires_plugin": "optimizer_constructor"
    },
    "abs": {
      "api": "jnp.abs"
    },
    "any": {
      "api": "praxis.base_layer.Any"
    },
    "data_ptr": {
      "api": "data",
      "requires_plugin": "method_to_property"
    },
    "exp": {
      "api": "praxis.layers.activations.Exp"
    },
    "permute_dims": {
      "api": "jnp.transpose",
      "requires_plugin": "pack_varargs"
    },
    "repeat": {
      "api": "praxis.layers.Repeat"
    },
    "size": {
      "api": "shape",
      "requires_plugin": "method_to_property"
    },
    "step": {
      "requires_plugin": "optimizer_step"
    },
    "tanh": {
      "api": "praxis.layers.Tanh"
    },
    "trace": {
      "api": "optax.trace"
    },
    "zero_grad": {
      "requires_plugin": "optimizer_zero_grad"
    }
  },
  "templates": {
    "fori_loop": "val = jax.lax.fori_loop({start}, {stop}, lambda i, val: {body}, {init_val})",
    "scan": "carry, stacked = jax.lax.scan(lambda c, x: {body}, {init}, {xs})"
  }
}